<?xml version="1.0" encoding="UTF-8"?>
<RDF xmlns:em="http://www.mozilla.org/2004/em-rdf#" xmlns="http://www.w3.org/1999/02/22-rdf-syntax-ns#">
  <Description about="urn:mozilla:install-manifest">
    <em:id>delicious-canonical-url@googlegroups.com</em:id>
    <em:name>Del.icio.us Canonical Url</em:name>
    <em:version>0.1</em:version>
    <em:creator>Chris Roos</em:creator>
    <em:description>Alters the behaviour of the delicious firefox extension so that bookmarking a page will use that pages canonical url (link rel="canonical") if one exists.  It falls back to the default behaviour of bookmarking the url of the document if the canonical url isn't specified.</em:description>
    <em:targetApplication>
      <Description>
        <em:id>{ec8030f7-c20a-464f-9b0e-13a3a9e97384}</em:id>
        <em:minVersion>2.0</em:minVersion>
        <em:maxVersion>3.0.*</em:maxVersion>
      </Description>
    </em:targetApplication>
  </Description>
</RDF>
