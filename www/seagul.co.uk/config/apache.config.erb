<VirtualHost *:80>
  ServerAdmin www-admin@seagul.co.uk
  ServerAdmin chris@seagul.co.uk
  ServerName seagul.co.uk
  ServerAlias www.seagul.co.uk
  RewriteEngine On
  # This Is Kent - Thanet news feed
  RewriteRule ^/thisiskent-thanet.atom$ http://thisiskent.seagul.co.uk/thanet.atom [R=301]
  # Postcodes
  RewriteRule ^/postcodes(.*)$ http://postcodes.seagul.co.uk$1 [R=301]
</VirtualHost>